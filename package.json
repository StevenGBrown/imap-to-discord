{
  "name": "imap-to-discord",
  "version": "0.1.0",
  "description": "AWS Lambda that retrieves emails from an IMAP mailbox and sends them to Discord",
  "keywords": [
    "aws",
    "email",
    "imap",
    "discord"
  ],
  "license": "MIT",
  "author": {
    "name": "Steven Brown",
    "email": "StevenGBrown@gmail.com"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenGBrown/imap-to-discord.git"
  },
  "scripts": {
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap",
    "deploy": "npm run lint && cdk deploy",
    "lint": "tsc --noEmit && eslint . --max-warnings 0 --ext .js,.ts",
    "start": "ts-node bin/start.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.360.0",
    "@aws-sdk/client-s3": "3.360.0",
    "@aws-sdk/lib-dynamodb": "3.44.0",
    "aws-cdk-lib": "2.124.0",
    "constructs": "10.3.0",
    "discord.js": "12.5.3",
    "escape-string-regexp": "4.0.0",
    "get-stream": "6.0.1",
    "imapflow": "1.0.144",
    "mailparser": "3.6.5",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.84",
    "@types/imapflow": "1.0.15",
    "@types/mailparser": "3.4.0",
    "@types/node": "20.11.10",
    "@types/ws": "8.2.1",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "aws-cdk": "2.124.0",
    "esbuild": "0.20.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-security": "1.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=7"
  }
}
