{
  "name": "imap-to-discord",
  "version": "0.1.0",
  "description": "AWS Lambda that retrieves emails from an IMAP mailbox and sends them to Discord",
  "keywords": [
    "aws",
    "email",
    "imap",
    "discord"
  ],
  "license": "MIT",
  "author": {
    "name": "Steven Brown",
    "email": "StevenGBrown@gmail.com"
  },
  "type": "module",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenGBrown/imap-to-discord.git"
  },
  "scripts": {
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap",
    "deploy": "npm run lint && cdk deploy",
    "lint": "tsc --noEmit && eslint . --max-warnings 0 --ext .js,.ts",
    "start": "tsx bin/start.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.622.0",
    "@aws-sdk/client-s3": "3.622.0",
    "@aws-sdk/lib-dynamodb": "3.44.0",
    "aws-cdk-lib": "2.219.0",
    "constructs": "10.4.2",
    "discord.js": "14.22.1",
    "escape-string-regexp": "4.0.0",
    "get-stream": "6.0.1",
    "imapflow": "1.0.198",
    "mailparser": "3.6.7",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "@eslint/compat": "1.4.0",
    "@types/aws-lambda": "8.10.84",
    "@types/imapflow": "1.0.15",
    "@types/mailparser": "3.4.0",
    "@types/node": "22.18.8",
    "@types/ws": "8.2.1",
    "aws-cdk": "2.1029.4",
    "esbuild": "0.25.0",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-security": "3.0.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=7"
  }
}
